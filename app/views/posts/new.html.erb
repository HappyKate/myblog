<h2>Новый пост</h2>
<%= form_for :post, url: posts_path do |f|%>
  <form>
    <div class="form-group">
      <label for="formGroupExampleInput">Название статьи</label>
      <%= f.text_field :title %>
    </div>
    <div class="form-group">
      <label for="formGroupExampleInput2">Статья</label>
      <%= f.text_area :body %>
    </div>
    <p><%= f.submit %></p>
  </form>
<%end%>